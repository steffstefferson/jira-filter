{
  "manifest_version": 3,
  "name": "Atlassian Jira Filter",
  "description": "Atlassian Jira Filter sprint or backlog by person",
  "action": {
    "default_icon": "icon48.png"
  },
  "version": "1.9.4.2",
  "author": "Stef KÃ¤ser",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "optional_host_permissions": ["https://*/*"],
  "permissions": ["activeTab"],
  "content_scripts": [
    {
	  "all_frames": true,
        "matches": [
          "https://*/*"
        ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}